-- å‰µå»ºè£œé–“å‹•ç•«å‡½æ•¸
local function animateButtonPosition(button, targetPosition, duration)
    local tweenInfo = TweenInfo.new(
        duration or 0.3,
        Enum.EasingStyle.Quart,
        Enum.EasingDirection.Out
    )
    local tween = TweenService:Create(button, tweenInfo, {Position = targetPosition})
    tween:Play()
    return tween
end

-- è‡ªå‹•æ”»æ“ŠåŠŸèƒ½
autoAttackBtn.MouseButton1Click:Connect(function()
    isAutoAttackEnabled = not isAutoAttackEnabled
    autoAttackBtn.Text = isAutoAttackEnabled and "è‡ªå‹•æ”»æ“Š: é–‹" or "è‡ªå‹•æ”»æ“Š: é—œ"
    
    if isAutoAttackEnabled then
        autoAttackLoop = task.spawn(function()
            while isAutoAttackEnabled do
                local closestPlayer = findClosestPlayer()
                if closestPlayer then
                    local args = {
                        [1] = closestPlayer.Character.Head
                    }
                    game:GetService("ReplicatedStorage").GeneralHit:FireServer(unpack(args))
                end
                task.wait(1)
            end
        end)
    elseif autoAttackLoop then
        task.cancel(autoAttackLoop)
        autoAttackLoop = nil
    end
end)

-- ä¸‹æ‹‰é¸å–®åŠŸèƒ½ - æ·»åŠ å‹•ç•«æ•ˆæœ
dropdownBtn.MouseButton1Click:Connect(function()
    dropdownMenu.Visible = not dropdownMenu.Visible
    
    if dropdownMenu.Visible then
        -- é¸å–®å±•é–‹æ™‚ï¼Œå°‡é–‹å§‹ä»»å‹™æŒ‰éˆ•ç§»å‹•åˆ°é¸å–®ä¸‹æ–¹
        animateButtonPosition(startTaskBtn, UDim2.new(0.5, -90, 0, 215))
    else
        -- é¸å–®é—œé–‰æ™‚ï¼Œå°‡é–‹å§‹ä»»å‹™æŒ‰éˆ•ç§»å›åŸä½
        animateButtonPosition(startTaskBtn, UDim2.new(0.5, -90, 0, 145))
    end
end)

option1.MouseButton1Click:Connect(function()
